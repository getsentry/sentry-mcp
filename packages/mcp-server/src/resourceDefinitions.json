[
  {
    "kind": "template",
    "name": "sentry-docs-platform",
    "description": "Sentry SDK documentation for {platform}",
    "mimeType": "text/markdown",
    "template": "https://docs.sentry.io/platforms/{platform}/",
    "variables": ["platform"]
  },
  {
    "kind": "template",
    "name": "sentry-docs-platform-guide",
    "description": "Sentry integration guide for {framework} on {platform}",
    "mimeType": "text/markdown",
    "template": "https://docs.sentry.io/platforms/{platform}/guides/{framework}/",
    "variables": ["platform", "framework"]
  },
  {
    "kind": "uri",
    "name": "sentry-mcp-about",
    "description": "Information about Sentry MCP service and its capabilities",
    "mimeType": "text/markdown",
    "uri": "sentry://about"
  }
]
